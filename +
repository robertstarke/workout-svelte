<script>
	import { page } from '$app/stores';
	import { getContext } from 'svelte';

	const exerciseLength = $page.url.searchParams.get('el');
	const restLength = $page.url.searchParams.get('rl');
	const repetitions = $page.url.searchParams.get('r');
	const setOrCycle = $page.url.searchParams.get('soc');

	const exercises = getContext('exercises');
</script>

<div>{exerciseLength}</div>
<div>{restLength}</div>
<div>{repetitions}</div>
<div>{setOrCycle}</div>

